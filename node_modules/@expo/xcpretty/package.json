{
  "name": "@expo/xcpretty",
  "description": "Parse and format xcodebuild logs",
  "version": "2.0.2",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "author": "Evan Bacon <bacon@expo.io> (https://github.com/evanbacon)",
  "homepage": "https://github.com/expo/expo-cli",
  "repository": {
    "type": "git",
